{
  "name": "clean-arquitecture-login-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent --colors",
    "test:unit": "npm test -- --watch",
    "test:staged": "npm test -- --passWithNoTests --findRelatedTests --noStackTrace",
    "test:ci": "npm run test:staged -- --coverage"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run test:staged",
      "standard --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisSky/clean-arquitecture-login-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuisSky/clean-arquitecture-login-api/issues"
  },
  "homepage": "https://github.com/LuisSky/clean-arquitecture-login-api#readme",
  "devDependencies": {
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "standard": "^14.3.4"
  }
}